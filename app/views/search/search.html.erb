<h1> Results </h1>

<% @results.each do |result| %>
  <ul class="result_card">
    <li>Name: <%= result.name %></li>
    <li>Address: <%= result.address %></li>
    <li>Fuel Type: <%= result.fuel_type %></li>
    <li>Distance: <%= result.distance %></li>
    <li>Access Times: <%= result.access_times %></li>
  </ul>
<% end %>
